<h1>Listing Attractions</h1>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Tickets</th>
        <th>Details</th>
    </tr>
    <% @attractions.each do |att| %>
        <tr>
            <td><%= att.name %></td>
            <td><%= att.tickets %></td>
            <td><%= link_to "Go on #{att.name}", attraction_path(att) %></td>
        </tr>
    <% end %>
</table>